<div class="container ">
  {{>menu}}
</div>
<div class="container">
  <div class="form-group pull-right">
    <input type="search" class="form-control ds-input" id="busca" name="busca" placeholder="Procure pelo cpf">
  </div>
  <table id="tabela" name"tabela" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th>id</th>
        <th>nome</th>
        <th>cpf</th>
        <th>Editar</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>{{id}}</td>
        <td>{{nome}}</td>
        <td>{{cpf}}</td>
        <td>
          <button class="btn btn-link"><i class='bx bxs-edit' id='{{_id}}'
              onclick="window.location.href='http://localhost:3000/usuario/controleAcesso/{{_id}}'">
            </i></button>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>